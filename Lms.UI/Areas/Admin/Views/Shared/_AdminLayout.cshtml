<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>
    <link rel="shortcut icon" href="~/AdminTemplates/images/favicon.ico">

    <!-- plugin css -->

    <link href="~/AdminTemplates/libs/admin-resources/jquery.vectormap/jquery-jvectormap-1.2.2.css" rel="stylesheet" type="text/css" />

    <!-- preloader css -->
    <link rel="stylesheet" href="~/AdminTemplates/css/preloader.min.css" type="text/css" />

    <!-- Bootstrap Css -->
    <link href="~/AdminTemplates/css/bootstrap.min.css" id="bootstrap-style" rel="stylesheet" type="text/css" />
    <!-- Icons Css -->
    <link href="~/AdminTemplates/css/icons.min.css" rel="stylesheet" type="text/css" />
    <!-- App Css-->
    <link href="~/AdminTemplates/css/app.min.css" id="app-style" rel="stylesheet" type="text/css" />
    @await RenderSectionAsync("links",false)
</head>
<body class="pace-done sidebar-enable">

    <div class="layout-wrapper">

        @await Component.InvokeAsync("Header");
        @await Component.InvokeAsync("Sidebar");
        <div class="main-content">
            <div class="page-content">
                <div class="container-fluid">
                    @RenderBody()
                </div>
            </div>
        </div>

    </div>


    <script src="~/AdminTemplates/libs/jquery/jquery.min.js"></script>
    <script src="~/AdminTemplates/libs/bootstrap/js/bootstrap.bundle.min.js"></script>
    <script src="~/AdminTemplates/libs/metismenu/metisMenu.min.js"></script>
    <script src="~/AdminTemplates/libs/simplebar/simplebar.min.js"></script>
    <script src="~/AdminTemplates/libs/node-waves/waves.min.js"></script>
    <script src="~/AdminTemplates/libs/feather-icons/feather.min.js"></script>
    <!-- pace js -->
    <script src="~/AdminTemplates/libs/pace-js/pace.min.js"></script>

    <!-- apexcharts -->
    <script src="~/AdminTemplates/libs/apexcharts/apexcharts.min.js"></script>

    <!-- Plugins js-->
    <script src="~/AdminTemplates/libs/admin-resources/jquery.vectormap/jquery-jvectormap-1.2.2.min.js"></script>
    <script src="~/AdminTemplates/libs/admin-resources/jquery.vectormap/maps/jquery-jvectormap-world-mill-en.js"></script>
    <!-- dashboard init -->
    <script src="~/AdminTemplates/js/pages/dashboard.init.js"></script>

    <script src="~/AdminTemplates/js/app.js"></script>
    @await RenderSectionAsync("scripts",false);
</body>
</html>
